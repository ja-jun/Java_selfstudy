package sec02.exam03;

public class Window {
	Button button1 = new Button();
	Button button2 = new Button();

	Button.OnClickListener listener = new button.OnClickListener() {
		@Override
		public void onClick() {
			System.out.println("전화를 겁니다.");
			
		}
	};
	
	window() {
		button1.setOnClickListener(listener);
	}


}



